{
  "name": "dora-metrics",
  "version": "1.0.0",
  "description": "DORA Metrics Dashboard - Track Deployment Frequency, Lead Time, Change Failure Rate, and Recovery Time",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --hot src/server.ts",
    "build": "bun build src/main.tsx --outdir=dist --minify",
    "test": "bun test",
    "test:unit": "bun test tests/unit",
    "test:integration": "bun test tests/integration",
    "typecheck": "bunx tsc --noEmit",
    "lint": "bunx ultracite check src",
    "lint:fix": "bunx ultracite fix src"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^20.0.10",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/bun": "latest",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.11",
    "chart.js": "^4.5.1",
    "date-fns": "^4.1.0",
    "octokit": "^5.0.5",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.0"
  }
}
